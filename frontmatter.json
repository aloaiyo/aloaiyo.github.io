{
  "$schema": "https://frontmatter.codes/frontmatter.schema.json",
  "frontMatter.taxonomy.contentTypes": [
    {
      "name": "default",
      "pageBundle": false,
      "previewPath": null,
      "fields": [
        {
          "title": "layout",
          "name": "layout",
          "type": "string"
        },
        {
          "title": "title",
          "name": "title",
          "type": "string"
        },
        {
          "title": "grand_parent",
          "name": "grand_parent",
          "type": "string"
        },
        {
          "title": "parent",
          "name": "parent",
          "type": "string"
        },
        {
          "title": "nav_order",
          "name": "nav_order",
          "type": "number"
        }
      ]
    }
  ],
  "frontMatter.framework.id": "jekyll",
  "frontMatter.content.publicFolder": "assets",
  "frontMatter.preview.host": "http://localhost:4000",
  "frontMatter.content.pageFolders": [
    {
      "title": "_site",
      "path": "[[workspace]]/_site"
    },
    {
      "title": "django",
      "path": "[[workspace]]/docs/programming/django"
    }
  ]
}